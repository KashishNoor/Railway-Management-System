<Page
    x:Class="RAILWAY_SYSTEM.Views.RevenuePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid x:Name="ContentArea">
        <ScrollViewer>
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Top">
                <Image x:Name="d" Source="/Assets/d.png" Height="250" Width="400" Margin="0 0 0 10"/>
                <TextBox x:Name="userText11" Width="300" Header="Train_ID" PlaceholderText="Enter ID" FontSize="15" HorizontalAlignment="Center" Margin="5 5 5 5"/>

            </StackPanel>
        </ScrollViewer>
    </Grid>
</Page>
